    "import rpa as r\n",
    "\n",
    "inptmes = \"6*sin(x) - x\"\n",
    "r.init(visual_automation = False, chrome_browser = True)\n",
    "r.url('https://www.wolframalpha.com/')\n",
    "r.type('//*[@class=\"_2Ejx\"]', 'd('+ inptmes + ')' + '/dx' + '[enter]')\n",
    "r.snap('//*[@class = \"_3fR4\"]', 'results.png')\n",
    "r.close()"
   ]
